{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "immutable"
  ],
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "complexity": [2, 5],
    "max-len": [1, 100, 2, {"ignoreComments": false}],
    "no-else-return": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/no-multi-comp": [2, { "ignoreStateless": true }],
    "immutable/no-let": 2,
    "immutable/no-mutation": 2,
    "arrow-body-style": 0,
    "no-use-before-define": 0
  }
}
